  "scripts": {
    "start:client": "react-scripts start",
    "start:server": "ts-node-dev --project server/tsconfig.json --transpile-only --no-notify --watch 'server/src/**/**.ts' --respawn server/src/server.ts",
    "build:client": "react-scripts build",
    "build:server": "cd server && tsc",
    "generate": "cd server && graphql-codegen --watch",
    "dev": "concurrently \"npm:start:server\" \"npm:generate\" \"npm:start:client\"",
    "test:client": "react-scripts test",
    "seed": "cd server && sequelize-cli db:seed:undo:all && sequelize-cli db:seed:all",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject"
